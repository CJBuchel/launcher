{
  "name": "fll-launcher",
  "version": "1.3.10",
  "description": "A Launcher to the FIRST LEGO League scoring system",
  "main": "app/main.js",
  "author": "Roy Shmueli",
  "license": "MIT",
  "engines": {
    "node": ">=6.11.1"
  },
  "scripts": {
    "modules:download": "node ./build/download-modules.js",
    "start": "electron .",
    "lint": "eslint app/**/*.js",
    "lint:fix": "yarn run lint -- --fix",
    "build:win": "electron-builder -w --dir",
    "build:linux": "electron-builder -l --dir",
    "get-internals": "node ./dev-scripts/get-all-internals.js",
    "postinstall": "electron-builder install-app-deps",
    "eslint": "eslint",
    "mhub-client": "mhub-client"
  },
  "build": {
    "appId": "com.fll-tools.launcher",
    "productName": "FIRST LEGO League Launcher",
    "artifactName": "fll-launcher-${version}",
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "app/**/*"
    ]
  },
  "dependencies": {
    "@keyv/sqlite": "^1.2.12",
    "adm-zip": "^0.4.7",
    "bluebird": "^3.5.0",
    "clipboardy": "^1.1.4",
    "ejs": "^2.5.8",
    "electron-debug": "^1.4.0",
    "electron-is-dev": "^0.3.0",
    "ip": "^1.1.5",
    "js-yaml": "^3.9.0",
    "keyv": "^3.0.0",
    "mhub": "^0.9.1",
    "mongodb": "^3.0.6",
    "request": "^2.81.0",
    "rimraf": "^2.6.2",
    "rotating-file-stream": "^1.3.2",
    "tar-fs": "^1.15.3"
  },
  "devDependencies": {
    "caporal": "^0.10.0",
    "electron": "^1.6.11",
    "electron-builder": "^19.16.2",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mkdirp": "^0.5.1",
    "unzip-stream": "^0.3.0"
  }
}
